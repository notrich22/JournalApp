@page
@model JournalWebApplication.Pages.UnauthorizedMethods.GetGroupsModel
@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StudyGroup[0].Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StudyGroup[0].GroupName)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.StudyGroup)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.GroupName)
                </td>
            </tr>
        }
    </tbody>
</table>